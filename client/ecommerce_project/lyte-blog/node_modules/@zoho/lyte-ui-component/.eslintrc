{
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "script"
    },
    "rules": {
        "argument-check": "warn",
        "check-html-usage": "warn",
        "combine-css-properties": "warn",
        "combine-properties": "warn",
        "combineJquery": "warn",
        "directly-select-with-id": "warn",
        "disable-comment": "warn",
        "invalid-window-reference": "warn",
        "jQuery-chaining": "warn",
        "layout-thrashing": "warn",
        "missed_error_res": "warn",
        "murphy-error": "warn",
        "no-ajaxloadtab": "warn",
        "no-animate": "warn",
        "no-async-false": "warn",
        "no-attribute-selectors": "warn",
        "no-body-events": "warn",
        "no-closure": "warn",
        "no-commoncode-in-ifelse": "warn",
        "no-complex-selector": "warn",
        "no-deprecated-fnc": "warn",
        "no-dummy-functions": "warn",
        "no-duplicate-null-check": "warn",
        "no-empty-function": "warn",
        "no-freezeLayer": "warn",
        "no-global-function-call": "warn",
        "no-global-variables": "error",
        "no-htmlText": "warn",
        "no-ifel": "warn",
        "no-jQuery-casting": "warn",
        "no-multipleDOMLookup": "warn",
        "no-objProto": "warn",
        "no-param-variable": "warn",
        "no-reserved-words": "warn",
        "no-unused-vars": "warn",
        "no-window-dimensions": "warn",
        "no-window-open": "warn",
        "ObjectCheck": "warn",
        "proper-usage-of-loop": "warn",
        "remove-check": "warn",
        "show-last": "warn",
        "typeof": "warn",
        "no-undef": "warn"
    },
    "globals": {
        "Lyte": false,
        "window": false,
        "document": false,
        "$L": false,
        "_lyteUiUtils": false
    }
}